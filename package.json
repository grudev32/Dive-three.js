{
  "name": "dive",
  "version": "0.0.1",
  "description": "Yuka showcase",
  "repository": "Mugen87/dive",
  "scripts": {
    "build": "rollup -c \"config/rollup/config.js\"",
    "build-prod": "rollup -c \"config/rollup/config.js\" && google-closure-compiler-js build/bundle.js > build/bundle.min.js",
    "dev": "concurrently \"rollup -c \"config/rollup/config.js\" -w\" \"http-server -c-1\"",
    "lint": "eslint src --ext .js",
    "start": "npm run dev"
  },
  "eslintConfig": {
    "extends": "mdcs"
  },
  "eslintIgnore": [
    "yuka.module.js",
    "three.module.js"
  ],
  "author": "Mugen87",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-mdcs": "^4.2.3",
    "google-closure-compiler": "^20190929.0.0",
    "http-server": "0.9.0",
    "rollup": "^1.26.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "three": "0.110.0",
    "yuka": "0.0.3"
  }
}
